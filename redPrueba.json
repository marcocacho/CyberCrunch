      },
      {
        "number": "10",
        "name": "Vlan 10",
        "interfaces": [
          "Gi 1/0"
        ]
      },
      {
        "number": "20",
        "name": "Vlan 20",
        "interfaces": [
          "Gi 2/0"
        ]
      }
    ]
  },
  "Alumno": {
