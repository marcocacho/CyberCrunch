{
  "R1": {
    "machineType": "router",
    "template": "c7200",
    "router": "cisco_ios",
    "interfaces": [
      {
        "iface": "fa0/0",
        "ip": "10.0.0.1",
        "netmask": "255.255.255.0"
      },
      {
        "iface": "fa1/0",
        "ip": "10.0.1.1",
        "netmask": "255.255.255.0",
        "nat": "inside"
      }
    ],
    "type": "rip",
    "routes": {
      "version": "2",
      "networks": [
        "10.0.0.0",
        "10.0.1.0"
      ]
    },
    "acls": [
      {
        "list": "10",
        "action": "permit",
        "origin": "10.0.1.0",
        "orNetmask": "255.255.255.0"
      },
      {
        "list": "110",
        "action": "deny",
        "origin": "10.0.2.14",
        "dest": "10.0.0.0",
        "destNetmask": "255.255.255.0",
        "socketTipe": "icmp",
        "protocol": "25"
      }
    ],
    "interfaces_acl": [
      {
        "interface": "fa0/0",
        "list_acl": [
          {
            "list": "10",
            "action": "out"
          },
           {
            "list": "110",
            "action": "out"
          }
        ]
      }
    ]
  },
  "R2":{"machineType": "router",
    "template": "c7200",
    "router": "cisco_ios",
    "interfaces": [
      {
        "iface": "fa0/0",
        "ip": "10.0.0.2",
        "netmask": "255.255.255.0"
      },
      {
        "iface": "fa1/0",
        "ip": "10.1.0.1",
        "netmask": "255.255.255.0",
        "nat": "inside"
      }
    ],
    "type": "rip",
    "routes": {
      "version": "2",
      "networks": [
        "10.0.0.0",
        "10.1.0.0",
        "10.2.0.0"
      ]
    }
  },
  "S1": {
    "machineType": "switch",
    "template": "Cisco IOSvL2 15.2(4.0.55)E",
    "switch": "cisco_ios",
    "vlans": [
      {
        "number": "trunk",
        "interfaces": [
          "Gi 0/0"
        ]
      },
      {
        "number": "10",
        "name": "Vlan 10",
        "interfaces": [
          "Gi 1/0"
        ]
      },
      {
        "number": "20",
        "name": "Vlan 20",
        "interfaces": [
          "Gi 2/0"
        ]
      }
    ]
  },
  "Alumno": {
    "machineType": "docker",
    "template": "ubuntu"
  },
    "D1": {
    "machineType": "docker",
    "template": "ubuntu"
  },
    "D2": {
    "machineType": "docker",
    "template": "ubuntu"
  },
  "connection_list": [
    [
      {
        "name": "R1",
        "interface": "FastEthernet0/0"
      },
      {
        "name": "R2",
        "interface": "f0/0"
      }
    ],
    [
      {
        "name": "R2",
        "interface": "FastEthernet2/0"
      },
      {
        "name": "S1",
        "interface": "Gi0/0"
      }
    ],
    [
      {
        "name": "D2",
        "interface": "eth0"
      },
      {
        "name": "S1",
        "interface": "Gi2/0"
      }
    ],
    [
      {
        "name": "D1",
        "interface": "eth0"
      },
      {
        "name": "S1",
        "interface": "Gi1/0"
      }
    ],
    [
      {
        "name": "R1",
        "interface": "f1/0"
      },
      {
        "name": "Alumno",
        "interface": "eth0"
      }
    ]
  ]
}

